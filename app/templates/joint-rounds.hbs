{{#if model}}
  <h3 class="text-center visible-xs">
    Rounds Played Together
  </h3>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>
          Course Name
        </th>
        <th>
          Date Played
        </th>
        <th>
        </th>
      </tr>
    </thead>
    <tbody>
      {{#each round in model}}
        <tr>
          <td>
            {{round.courseName}}
          </td>
          <td>
            {{display-date round.createdAt}}
          </td>
          <td>
            {{#link-to "round" round}}
              Recap
            {{/link-to}}
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
{{else}}
  <p>
    You two haven't played any rounds together! Get discin'!
  </p>
{{/if}}
